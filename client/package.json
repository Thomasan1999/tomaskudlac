{
    "version": "0.0.0",
    "scripts": {
        "serve": "vite",
        "build": "vite build --emptyOutDir",
        "test:all": "npm run test:e2e:all && npm run test:unit:all",
        "test:e2e": "jest --selectProjects e2e --bail --findRelatedTests",
        "test:e2e:all": "jest --selectProjects e2e --bail --passWithNoTests",
        "test:unit": "jest --selectProjects unit --bail --findRelatedTests",
        "test:unit:all": "jest --selectProjects unit --passWithNoTests",
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,vue}": [
            "eslint"
        ],
        "**/*.ts": [
            "npm run test:unit"
        ]
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "1.3.0",
        "@fortawesome/free-brands-svg-icons": "6.0.0",
        "@fortawesome/free-regular-svg-icons": "6.0.0",
        "@fortawesome/free-solid-svg-icons": "6.0.0",
        "@fortawesome/vue-fontawesome": "3.0.0-5",
        "cors": "2.8.5",
        "dayjs": "1.10.8",
        "express": "4.17.3",
        "express-static-gzip": "2.1.5",
        "lodash": "4.17.21",
        "pinia": "2.0.11",
        "scroll-behavior-polyfill": "2.0.13",
        "vue": "3.2.31",
        "vue-gtag": "2.0.1",
        "vue-router": "4.0.13"
    },
    "devDependencies": {
        "@types/expect-puppeteer": "^4.4.7",
        "@types/jest": "27.0.3",
        "@types/jest-environment-puppeteer": "^5.0.0",
        "@types/lodash": "4.14.178",
        "@types/puppeteer": "^5.4.5",
        "@typescript-eslint/eslint-plugin": "5.12.1",
        "@typescript-eslint/parser": "5.14.0",
        "@vitejs/plugin-vue": "2.2.2",
        "@vue/compiler-sfc": "3.2.31",
        "@vue/eslint-config-airbnb": "6.0.0",
        "@vue/test-utils": "2.0.0-rc.16",
        "dart-sass": "1.25.0",
        "eslint": "8.10.0",
        "eslint-import-resolver-typescript": "2.5.0",
        "eslint-plugin-import": "2.25.4",
        "eslint-plugin-vue": "8.5.0",
        "jest": "26.6.3",
        "jest-puppeteer": "^6.1.0",
        "lint-staged": "12.3.7",
        "puppeteer": "13.4.0",
        "sass": "1.49.9",
        "sass-loader": "12.6.0",
        "ts-essentials": "9.1.2",
        "ts-jest": "26.5.6",
        "ts-node": "10.7.0",
        "typescript": "4.5.5",
        "vite": "2.8.6",
        "vite-plugin-eslint": "1.3.0",
        "vite-plugin-html": "3.0.6",
        "vite-plugin-pwa": "0.11.13",
        "vue-jest": "5.0.0-alpha.10",
        "vue-tsc": "0.32.1"
    }
}
