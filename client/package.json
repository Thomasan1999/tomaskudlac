{
    "version": "0.0.0",
    "scripts": {
        "serve": "vite",
        "build": "vite build --emptyOutDir",
        "test:all": "npm run test:e2e:all && npm run test:unit:all",
        "test:e2e": "jest --selectProjects e2e --bail --findRelatedTests",
        "test:e2e:all": "jest --selectProjects e2e --bail --passWithNoTests",
        "test:unit": "jest --selectProjects unit --bail --findRelatedTests",
        "test:unit:all": "jest --selectProjects unit --passWithNoTests",
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,vue}": [
            "eslint"
        ],
        "**/*.ts": [
            "npm run test:unit"
        ]
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "1.2.36",
        "@fortawesome/free-brands-svg-icons": "5.15.4",
        "@fortawesome/free-regular-svg-icons": "5.15.4",
        "@fortawesome/free-solid-svg-icons": "5.15.4",
        "@fortawesome/vue-fontawesome": "3.0.0-5",
        "cors": "2.8.5",
        "dayjs": "1.10.7",
        "express": "4.17.1",
        "express-static-gzip": "2.1.1",
        "lodash": "4.17.21",
        "pinia": "2.0.5",
        "scroll-behavior-polyfill": "2.0.13",
        "vue": "3.2.23",
        "vue-gtag": "2.0.1",
        "vue-router": "4.0.12"
    },
    "devDependencies": {
        "@types/jest": "27.0.3",
        "@types/lodash": "4.14.177",
        "@types/expect-puppeteer": "^4.4.7",
        "@types/jest-environment-puppeteer": "^4.4.1",
        "@types/puppeteer": "^5.4.4",
        "@typescript-eslint/eslint-plugin": "4.33.0",
        "@typescript-eslint/parser": "4.33.0",
        "@vitejs/plugin-vue": "1.10.1",
        "@vue/compiler-sfc": "3.2.23",
        "@vue/eslint-config-airbnb": "5.3.0",
        "@vue/test-utils": "2.0.0-rc.16",
        "dart-sass": "1.25.0",
        "eslint": "8.3.0",
        "eslint-import-resolver-typescript": "2.5.0",
        "eslint-plugin-import": "2.25.3",
        "eslint-plugin-vue": "8.1.1",
        "jest": "26.6.3",
        "lint-staged": "12.1.2",
        "sass": "1.44.0",
        "sass-loader": "12.3.0",
        "jest-puppeteer": "^6.0.0",
        "puppeteer": "^10.4.0",
        "ts-essentials": "9.0.0",
        "ts-jest": "26.5.6",
        "ts-node": "10.4.0",
        "typescript": "4.5.2",
        "vite": "2.6.14",
        "vite-plugin-eslint": "1.3.0",
        "vite-plugin-html": "2.1.1",
        "vite-plugin-pwa": "0.11.9",
        "vue-jest": "5.0.0-alpha.10",
        "vue-tsc": "0.29.8"
    }
}
