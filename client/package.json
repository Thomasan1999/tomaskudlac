{
    "version": "0.0.0",
    "scripts": {
        "serve": "vite",
        "build": "vite build --emptyOutDir",
        "test:all": "npm run test:e2e:all && npm run test:unit:all",
        "test:e2e": "jest --selectProjects e2e --bail --findRelatedTests",
        "test:e2e:all": "jest --selectProjects e2e --bail --passWithNoTests",
        "test:unit": "jest --selectProjects unit --bail --findRelatedTests",
        "test:unit:all": "jest --selectProjects unit --passWithNoTests",
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,vue}": [
            "eslint"
        ],
        "!(src/tests/e2e/**/*)**/*.ts": [
            "npm run test:unit"
        ],
        "src/tests/e2e/**/*.ts": [
            "npm run test:e2e"
        ]
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "6.2.0",
        "@fortawesome/free-brands-svg-icons": "6.2.0",
        "@fortawesome/free-regular-svg-icons": "6.2.0",
        "@fortawesome/free-solid-svg-icons": "6.2.0",
        "@fortawesome/vue-fontawesome": "3.0.1",
        "cors": "2.8.5",
        "dayjs": "1.11.6",
        "express": "4.18.2",
        "express-static-gzip": "2.1.7",
        "lodash": "4.17.21",
        "pinia": "2.0.23",
        "scroll-behavior-polyfill": "2.0.13",
        "vue": "3.2.41",
        "vue-gtag": "2.0.1",
        "vue-router": "4.1.6"
    },
    "devDependencies": {
        "@pinia/testing": "0.0.14",
        "@types/expect-puppeteer": "^5.0.1",
        "@types/jest": "29.2.0",
        "@types/jest-environment-puppeteer": "^5.0.2",
        "@types/lodash": "4.14.186",
        "@types/puppeteer": "5.4.7",
        "@typescript-eslint/eslint-plugin": "5.41.0",
        "@typescript-eslint/parser": "5.41.0",
        "@vitejs/plugin-vue": "3.2.0",
        "@vue/compiler-sfc": "3.2.41",
        "@vue/eslint-config-airbnb": "7.0.0",
        "@vue/test-utils": "2.2.1",
        "@vue/vue3-jest": "29.1.1",
        "dart-sass": "1.25.0",
        "eslint": "8.26.0",
        "eslint-import-resolver-typescript": "3.5.2",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-vue": "9.6.0",
        "jest": "29.2.2",
        "jest-environment-jsdom": "29.2.2",
        "jest-puppeteer": "^6.1.1",
        "lint-staged": "13.0.3",
        "puppeteer": "19.2.0",
        "sass": "1.55.0",
        "sass-loader": "13.1.0",
        "ts-essentials": "9.3.0",
        "ts-jest": "29.0.3",
        "ts-node": "10.9.1",
        "typescript": "4.8.4",
        "vite": "3.2.1",
        "vite-plugin-eslint": "1.8.1",
        "vite-plugin-html": "3.2.0",
        "vite-plugin-pwa": "0.13.1",
        "vue-tsc": "1.0.9"
    }
}
