{
    "version": "0.0.0",
    "scripts": {
        "serve": "vite",
        "build": "vite build --emptyOutDir",
        "test:all": "npm run test:e2e:all && npm run test:unit:all",
        "test:e2e": "jest --selectProjects e2e --bail --findRelatedTests",
        "test:e2e:all": "jest --selectProjects e2e --bail --passWithNoTests",
        "test:unit": "jest --selectProjects unit --bail --findRelatedTests",
        "test:unit:all": "jest --selectProjects unit --passWithNoTests",
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,vue}": [
            "eslint"
        ],
        "**/*.ts": [
            "npm run test:unit"
        ]
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "6.1.1",
        "@fortawesome/free-brands-svg-icons": "6.1.1",
        "@fortawesome/free-regular-svg-icons": "6.1.1",
        "@fortawesome/free-solid-svg-icons": "6.1.1",
        "@fortawesome/vue-fontawesome": "3.0.0-5",
        "cors": "2.8.5",
        "dayjs": "1.11.2",
        "express": "4.18.1",
        "express-static-gzip": "2.1.7",
        "lodash": "4.17.21",
        "pinia": "2.0.14",
        "scroll-behavior-polyfill": "2.0.13",
        "vue": "3.2.36",
        "vue-gtag": "2.0.1",
        "vue-router": "4.0.15"
    },
    "devDependencies": {
        "@types/expect-puppeteer": "^4.4.7",
        "@types/jest": "26.0.24",
        "@types/jest-environment-puppeteer": "^5.0.2",
        "@types/lodash": "4.14.182",
        "@types/puppeteer": "^5.4.6",
        "@typescript-eslint/eslint-plugin": "5.27.0",
        "@typescript-eslint/parser": "5.27.0",
        "@vitejs/plugin-vue": "2.3.3",
        "@vue/compiler-sfc": "3.2.36",
        "@vue/eslint-config-airbnb": "6.0.0",
        "@vue/test-utils": "2.0.0",
        "dart-sass": "1.25.0",
        "eslint": "8.17.0",
        "eslint-import-resolver-typescript": "2.7.1",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-vue": "9.1.0",
        "jest": "26.6.3",
        "jest-puppeteer": "^6.1.0",
        "lint-staged": "13.0.0",
        "pug": "3.0.2",
        "puppeteer": "15.3.0",
        "sass": "1.52.2",
        "sass-loader": "13.0.0",
        "ts-essentials": "9.1.2",
        "ts-jest": "26.5.6",
        "ts-node": "10.8.1",
        "typescript": "4.7.3",
        "vite": "2.9.9",
        "vite-plugin-eslint": "1.6.1",
        "vite-plugin-html": "3.2.0",
        "vite-plugin-pwa": "0.12.0",
        "vue-jest": "5.0.0-alpha.10",
        "vue-tsc": "0.36.0"
    }
}
