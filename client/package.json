{
    "version": "0.0.0",
    "scripts": {
        "serve": "run-p serve:vite build:vue-dts:watch",
        "serve:vite": "vite",
        "build": "vite build --emptyOutDir",
        "build:vue-dts": "run-s -c build:vue-dts:base build:vue-dts:filter",
        "build:vue-dts:base": "vue-tsc -p tsconfig.build-vue-dts.json",
        "build:vue-dts:filter": "node --loader ts-node/esm scripts/filterDtsFiles.ts",
        "build:vue-dts:watch": "npm run build:vue-dts:base -- --watch",
        "test:run": "vitest --run",
        "test:watch": "vitest -w",
        "lint": "eslint",
        "format": "prettier . -write",
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,vue}": [
            "npm run lint"
        ],
        "src/**/*.spec.ts": [
            "npm run test:run"
        ]
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "6.6.0",
        "@fortawesome/free-brands-svg-icons": "6.6.0",
        "@fortawesome/free-regular-svg-icons": "6.6.0",
        "@fortawesome/free-solid-svg-icons": "6.6.0",
        "@fortawesome/vue-fontawesome": "3.0.8",
        "cors": "2.8.5",
        "dayjs": "1.11.12",
        "lodash": "4.17.21",
        "pinia": "2.2.2",
        "vue": "3.4.38",
        "vue-gtag": "2.0.1",
        "vue-router": "4.4.3"
    },
    "devDependencies": {
        "@pinia/testing": "0.1.5",
        "@types/lodash": "4.17.7",
        "@typescript-eslint/eslint-plugin": "8.1.0",
        "@typescript-eslint/parser": "8.1.0",
        "@vitejs/plugin-vue": "5.1.2",
        "@vue/compiler-sfc": "3.4.38",
        "@vue/eslint-config-airbnb": "8.0.0",
        "@vue/test-utils": "2.4.6",
        "autoprefixer": "10.4.20",
        "eslint": "9.9.0",
        "eslint-import-resolver-typescript": "3.6.1",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-vue": "9.27.0",
        "glob": "11.0.0",
        "lint-staged": "15.2.9",
        "npm-run-all": "4.1.5",
        "puppeteer": "23.1.0",
        "sass": "1.77.8",
        "tailwindcss": "3.4.10",
        "ts-essentials": "10.0.2",
        "ts-node": "10.9.2",
        "typescript": "5.5.4",
        "typescript-eslint": "8.1.0",
        "vite": "5.4.1",
        "vite-plugin-eslint": "1.8.1",
        "vite-plugin-html": "3.2.2",
        "vite-plugin-pwa": "0.20.1",
        "vitest": "2.0.5",
        "vue-tsc": "2.1.4"
    },
    "type": "module"
}
