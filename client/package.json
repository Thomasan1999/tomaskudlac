{
    "name": "client",
    "version": "0.0.0",
    "scripts": {
        "build": "vite build --emptyOutDir",
        "format:base": "prettier .",
        "format": "npm run format:base -- --write",
        "format:check": "npm run format:base -- --check",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "serve": "vite",
        "test:coverage:base": "npm run test:run:base -- --coverage",
        "test:coverage": "run-p serve test:coverage:base --race",
        "test:run:base": "vitest --run",
        "test:run": "run-p serve test:run:base --race",
        "test:ui:base": "npm run test:watch:base -- --ui",
        "test:ui": "run-p serve test:ui:base",
        "test:watch:base": "vitest -w",
        "test:watch": "run-p serve test:watch:base",
        "type-check": "vue-tsc --noEmit"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "7.1.0",
        "@fortawesome/free-brands-svg-icons": "7.1.0",
        "@fortawesome/free-regular-svg-icons": "7.1.0",
        "@fortawesome/free-solid-svg-icons": "7.1.0",
        "@fortawesome/vue-fontawesome": "3.1.2",
        "cors": "2.8.5",
        "dayjs": "1.11.19",
        "lodash": "4.17.21",
        "pinia": "3.0.4",
        "vue": "3.5.26",
        "vue-gtag": "3.6.3",
        "vue-router": "4.6.4"
    },
    "devDependencies": {
        "@pinia/testing": "1.0.3",
        "@tailwindcss/vite": "4.1.18",
        "@types/lodash": "4.17.21",
        "@typescript-eslint/eslint-plugin": "8.51.0",
        "@typescript-eslint/parser": "8.51.0",
        "@vitejs/plugin-vue": "6.0.3",
        "@vitest/coverage-v8": "4.0.16",
        "@vitest/ui": "4.0.16",
        "@vue/compiler-sfc": "3.5.26",
        "@vue/language-server": "3.2.1",
        "@vue/test-utils": "2.4.6",
        "eslint": "9.39.2",
        "eslint-plugin-vue": "10.6.2",
        "jsdom": "27.4.0",
        "puppeteer": "24.34.0",
        "tailwindcss": "4.1.18",
        "ts-essentials": "10.1.1",
        "ts-node": "10.9.2",
        "typescript": "5.9.3",
        "typescript-eslint": "8.51.0",
        "vite": "7.3.0",
        "vite-plugin-eslint": "1.8.1",
        "vite-plugin-html": "3.2.2",
        "vite-plugin-pwa": "1.2.0",
        "vitest": "4.0.16",
        "vue-eslint-parser": "10.2.0",
        "vue-tsc": "3.2.1"
    }
}
