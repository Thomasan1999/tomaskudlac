{
    "version": "0.0.0",
    "scripts": {
        "serve": "vite",
        "build": "vite build --emptyOutDir",
        "test:all": "npm run test:e2e:all && npm run test:unit:all",
        "test:e2e": "jest --selectProjects e2e --bail --findRelatedTests",
        "test:e2e:all": "jest --selectProjects e2e --bail --passWithNoTests",
        "test:unit": "jest --selectProjects unit --bail --findRelatedTests",
        "test:unit:all": "jest --selectProjects unit --passWithNoTests",
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,vue}": [
            "eslint"
        ],
        "!(src/tests/e2e/**/*)**/*.ts": [
            "npm run test:unit"
        ],
        "src/tests/e2e/**/*.ts": [
            "npm run test:e2e"
        ]
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "6.4.2",
        "@fortawesome/free-brands-svg-icons": "6.4.2",
        "@fortawesome/free-regular-svg-icons": "6.4.2",
        "@fortawesome/free-solid-svg-icons": "6.4.2",
        "@fortawesome/vue-fontawesome": "3.0.3",
        "cors": "2.8.5",
        "dayjs": "1.11.9",
        "express": "4.18.2",
        "express-static-gzip": "2.1.7",
        "lodash": "4.17.21",
        "pinia": "2.1.6",
        "recaptcha-v3": "1.10.0",
        "scroll-behavior-polyfill": "2.0.13",
        "vue": "3.3.4",
        "vue-gtag": "2.0.1",
        "vue-router": "4.2.4"
    },
    "devDependencies": {
        "@pinia/testing": "0.1.3",
        "@types/expect-puppeteer": "5.0.3",
        "@types/jest": "29.2.0",
        "@types/jest-environment-puppeteer": "^5.0.2",
        "@types/lodash": "4.14.197",
        "@typescript-eslint/eslint-plugin": "6.6.0",
        "@typescript-eslint/parser": "6.6.0",
        "@vitejs/plugin-vue": "4.3.4",
        "@vue/compiler-sfc": "3.3.4",
        "@vue/eslint-config-airbnb": "7.0.0",
        "@vue/test-utils": "2.4.1",
        "@vue/vue3-jest": "29.1.1",
        "eslint": "8.48.0",
        "eslint-import-resolver-typescript": "3.6.0",
        "eslint-plugin-import": "2.28.1",
        "eslint-plugin-vue": "9.17.0",
        "jest": "29.2.2",
        "jest-environment-jsdom": "29.2.2",
        "jest-puppeteer": "^6.1.1",
        "lint-staged": "14.0.1",
        "puppeteer": "21.1.1",
        "sass": "1.66.1",
        "sass-loader": "13.3.2",
        "ts-essentials": "9.4.0",
        "ts-jest": "29.0.3",
        "ts-node": "10.9.1",
        "typescript": "5.2.2",
        "vite": "4.4.9",
        "vite-plugin-eslint": "1.8.1",
        "vite-plugin-html": "3.2.0",
        "vite-plugin-pwa": "0.16.4",
        "vue-tsc": "1.8.8"
    }
}
