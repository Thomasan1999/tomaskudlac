{
    "version": "0.0.0",
    "scripts": {
        "serve": "vite",
        "build": "vite build --emptyOutDir",
        "test:all": "npm run test:e2e:all && npm run test:unit:all",
        "test:e2e": "jest --selectProjects e2e --bail --findRelatedTests",
        "test:e2e:all": "jest --selectProjects e2e --bail --passWithNoTests",
        "test:unit": "jest --selectProjects unit --bail --findRelatedTests",
        "test:unit:all": "jest --selectProjects unit --passWithNoTests",
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,vue}": [
            "eslint"
        ],
        "**/*.ts": [
            "npm run test:unit"
        ]
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "6.1.2",
        "@fortawesome/free-brands-svg-icons": "6.1.2",
        "@fortawesome/free-regular-svg-icons": "6.1.2",
        "@fortawesome/free-solid-svg-icons": "6.1.2",
        "@fortawesome/vue-fontawesome": "3.0.1",
        "cors": "2.8.5",
        "dayjs": "1.11.5",
        "express": "4.18.1",
        "express-static-gzip": "2.1.7",
        "lodash": "4.17.21",
        "pinia": "2.0.20",
        "scroll-behavior-polyfill": "2.0.13",
        "vue": "3.2.37",
        "vue-gtag": "2.0.1",
        "vue-router": "4.1.3"
    },
    "devDependencies": {
        "@pinia/testing": "0.0.14",
        "@types/expect-puppeteer": "^5.0.1",
        "@types/jest": "28.1.7",
        "@types/jest-environment-puppeteer": "^5.0.2",
        "@types/lodash": "4.14.184",
        "@types/puppeteer": "^5.4.6",
        "@typescript-eslint/eslint-plugin": "5.33.1",
        "@typescript-eslint/parser": "5.33.1",
        "@vitejs/plugin-vue": "3.0.3",
        "@vue/compiler-sfc": "3.2.37",
        "@vue/eslint-config-airbnb": "7.0.0",
        "@vue/test-utils": "2.0.2",
        "@vue/vue3-jest": "28.1.0",
        "dart-sass": "1.25.0",
        "eslint": "8.22.0",
        "eslint-import-resolver-typescript": "3.4.2",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-vue": "9.3.0",
        "jest": "28.1.3",
        "jest-environment-jsdom": "28.1.3",
        "jest-puppeteer": "^6.1.1",
        "lint-staged": "13.0.3",
        "puppeteer": "14.2.1",
        "sass": "1.54.5",
        "sass-loader": "13.0.2",
        "ts-essentials": "9.2.0",
        "ts-jest": "28.0.8",
        "ts-node": "10.9.1",
        "typescript": "4.7.4",
        "vite": "3.0.9",
        "vite-plugin-eslint": "1.8.1",
        "vite-plugin-html": "3.2.0",
        "vite-plugin-pwa": "0.12.3",
        "vue-tsc": "0.40.1"
    }
}
