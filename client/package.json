{
    "version": "0.0.0",
    "scripts": {
        "build": "vite build --emptyOutDir",
        "format:base": "prettier .",
        "format": "npm run format:base -- --write",
        "format:check": "npm run format:base -- --check",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "serve": "vite",
        "test:coverage:base": "npm run test:run:base -- --coverage",
        "test:coverage": "run-p serve test:coverage:base --race",
        "test:run:base": "vitest --run",
        "test:run": "run-p serve test:run:base --race",
        "test:ui:base": "npm run test:watch:base -- --ui",
        "test:ui": "run-p serve test:ui:base",
        "test:watch:base": "vitest -w",
        "test:watch": "run-p serve test:watch:base",
        "type-check": "vue-tsc --noEmit"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "6.6.0",
        "@fortawesome/free-brands-svg-icons": "6.6.0",
        "@fortawesome/free-regular-svg-icons": "6.6.0",
        "@fortawesome/free-solid-svg-icons": "6.6.0",
        "@fortawesome/vue-fontawesome": "3.0.8",
        "cors": "2.8.5",
        "dayjs": "1.11.12",
        "lodash": "4.17.21",
        "pinia": "2.2.2",
        "vue": "3.5.26",
        "vue-gtag": "3.6.3",
        "vue-router": "4.6.4"
    },
    "devDependencies": {
        "@pinia/testing": "0.1.5",
        "@types/lodash": "4.17.7",
        "@typescript-eslint/eslint-plugin": "8.1.0",
        "@typescript-eslint/parser": "8.1.0",
        "@vitejs/plugin-vue": "6.0.3",
        "@vitest/coverage-v8": "4.0.16",
        "@vitest/ui": "4.0.16",
        "@vue/compiler-sfc": "3.5.5",
        "@vue/language-server": "2.1.6",
        "@vue/test-utils": "2.4.6",
        "autoprefixer": "10.4.20",
        "eslint": "9.9.0",
        "eslint-plugin-vue": "9.27.0",
        "jsdom": "27.4.0",
        "puppeteer": "24.34.0",
        "sass": "1.97.1",
        "tailwindcss": "3.4.10",
        "ts-essentials": "10.1.1",
        "ts-node": "10.9.2",
        "typescript": "5.9.3",
        "typescript-eslint": "8.1.0",
        "vite": "7.3.0",
        "vite-plugin-eslint": "1.8.1",
        "vite-plugin-html": "3.2.2",
        "vite-plugin-pwa": "1.2.0",
        "vitest": "4.0.16",
        "vue-tsc": "3.2.1"
    }
}
